{
    "manifest_version": 3,
    "name": "ArthasMod",
    "version": "1.0",
    "description": "Backend optimizations and bugfixes for isy.ksr.ch made by Arthas1811",
    "host_permissions": [
        "*://isy.ksr.ch/*",
        "*://isy-api.ksr.ch/*",
        "*://*.ksr.ch/*"
    ],
    "permissions": [
        "cookies"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://isy.ksr.ch/*"
            ],
            "js": [
                "main-world-cache.js"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        },
        {
            "matches": [
                "*://isy.ksr.ch/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
